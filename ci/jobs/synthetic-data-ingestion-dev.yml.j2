jobs:
  - name: synthetic-data-ingestion-dev
    plan:
      - aggregate:
        - get: aws-analytical-dataset-generation
          trigger: true
        - get: aws-internal-compute
          trigger: false
        - get: aws-sdx
          trigger: false
        - get: aws-ingestion
          trigger: false
      - .: (( inject meta.plan.terraform-bootstrap ))
      - .: (( inject meta.plan.terraform-output-internal-compute ))
      - .: (( inject meta.plan.terraform-output-ingest ))
      - .: (( inject meta.plan.terraform-output-sdx ))
      - .: (( inject meta.plan.synthetic-data-ingestion ))
        config:
          params:
            AWS_ACC: "{{accounts['development']}}"
